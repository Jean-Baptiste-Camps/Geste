                  id="otinel-hi-rend-obligatoire-transcriptions-constraint-assert-3">
            <rule context="tei:hi">
               <s:assert xmlns:xi="http://www.w3.org/2001/XInclude"
                         xmlns="http://www.tei-c.org/ns/1.0"
                         xmlns:s="http://purl.oclc.org/dsdl/schematron"
                         test="not(/tei:TEI[@type='ms']) or (/tei:TEI[@type='ms'] and not(//tei:hi[not(@rend)]) )">l'attribut @rend est obligatoire pour l'élément hi dans les
                                    transcriptions de manuscrit</s:assert>
            </rule>
         </pattern>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.global.attribute.n"/>
         <ref name="att.global.attribute.xmllang"/>
         <ref name="att.global.linking.attribute.corresp"/>
         <ref name="att.global.linking.attribute.sameAs"/>
         <ref name="att.global.analytic.attribute.ana"/>
         <ref name="att.global.responsibility.attribute.cert"/>
         <ref name="att.global.responsibility.attribute.resp"/>
         <ref name="att.global.source.attribute.source"/>
         <ref name="att.written.attributes"/>
         <optional>
            <attribute name="rend">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(interprétation) indique comment l'élément en question a été rendu ou présenté dans le texte source</a:documentation>
               <list>
                  <oneOrMore>
                     <choice>
                        <value>superscript</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">spécifie que le contenu est suscrit, lorsqu'aucun élément plus spécifique ne peut être utilisé.</a:documentation>
                        <value>initiale</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">décrit une initiale de tout type</a:documentation>
                        <value>filigr</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">spécifie qu'un élément est filigrané</a:documentation>
                        <value>peint</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">spécifie qu'un élément est peint</a:documentation>
                        <value>litteraElongata</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">décrit une littera elongata</a:documentation>
                        <value>litteraNotabilior</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">décrit une littera notabilior</a:documentation>
                        <value>detach</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">précise qu'une initiale est détachée de ce qui suit par une espace</a:documentation>
                        <value>2l</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">spécifie qu'une initiale occupe 2 l. de réglure</a:documentation>
                        <value>4l</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">spécifie qu'une initiale occupe 4 l. de réglure</a:documentation>
                        <value>5l</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">spécifie qu'une initiale occupe 5 l. de réglure</a:documentation>
                     </choice>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="quote">
