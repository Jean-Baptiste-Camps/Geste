      </optional>
   </define>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="otinel-att.datable-calendar-calendar-constraint-rule-4">
      <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:rng="http://relaxng.org/ns/structure/1.0"
                xmlns:xi="http://www.w3.org/2001/XInclude"
                xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:s="http://purl.oclc.org/dsdl/schematron"
                context="tei:*[@calendar]">
         <sch:assert test="string-length(.) gt 0"> @calendar indicates the system or calendar to
              which the date represented by the content of this element belongs, but this
              <sch:name/> element has no textual content.</sch:assert>
      </sch:rule>
   </pattern>
   <define name="att.datable.attribute.period">
      <optional>
         <attribute name="period">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">fournit un pointeur vers un emplacement donné définissant une période de temps nommée durant laquelle l'item concerné s'inscrit.</a:documentation>
            <data type="anyURI"/>
         </attribute>
      </optional>
   </define>
   <define name="att.docStatus.attributes">
      <ref name="att.docStatus.attribute.status"/>
   </define>
   <define name="att.docStatus.attribute.status">
      <optional>
         <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                    name="status"
                    a:defaultValue="draft">
            <a:documentation>describes the status of a document either currently or, when associated with a dated element, at the time indicated.</a:documentation>
            <choice>
               <value>draft</value>
               <a:documentation>(brouillon) le document est dans un état préparatoire</a:documentation>
               <value>frozen</value>
               <a:documentation>(gelé) le document est gelé dans l'état actuel et aucune modification ne doit être faite</a:documentation>
               <value>deprecated</value>
               <a:documentation>(déprécié) le document est déprécié pour une raison ou une autre et ne doit plus être utilisé</a:documentation>
               <value>proposed</value>
               <a:documentation>(proposé) le document est dans un état pré-final et est proposé pour publication ; il doit répondre à la version plus complète du modèle</a:documentation>
               <value>published</value>
               <a:documentation>(publié) le document a été approuvé et est publié dans la version actuelle, qui est considérée comme définitive</a:documentation>
            </choice>
         </attribute>
      </optional>
   </define>
