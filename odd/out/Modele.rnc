    ## specifie où cet item se trouve.
    attribute place {
      list {
        (
         ## (au-dessus) l'ajout a été fait au-dessus de la ligne
         "above"
         | 
           ## (à la fin) l'ajout a été fait à la fin de la ligne
           "end"
         | 
           ## (dans la ligne) l'ajout a été fait dans la ligne, par exemple à la suite d'une portion supprimée
           "inline"
         | 
           ## (en lieu et place) l'ajout a été fait là où il doit s'insérer, par exemple dans une portion laissée blanche ou par dessus un texte gratté
           "inplace"
         | 
           ## (marge) l'ajout a été fait dans la marge
