  attribute ana {
    list { xsd:anyURI+ }
  }?
w =
  
  ## (mot) représente un mot grammatical (pas nécessairement orthographique) [17.1. Linguistic Segment Categories]
  element w {
    (text
     | model.gLike
     | seg
     | gap
     | space
     | model.hiLike
     | model.milestoneLike
     | model.noteLike
     | choice
     | subst
     | model.pPart.transcriptional)+,
    att.global.attribute.xmlid,
    att.global.attribute.xmllang,
    att.global.linking.attribute.synch,
    att.global.linking.attribute.sameAs,
    att.global.linking.attribute.copyOf,
    att.global.linking.attribute.next,
    att.global.linking.attribute.prev,
    att.global.linking.attribute.exclude,
    att.global.linking.attribute.select,
    att.global.analytic.attribute.ana,
    att.global.responsibility.attribute.cert,
    att.global.responsibility.attribute.resp,
    att.segLike.attributes,
    
    ## (interprétation) indique comment l'élément en question a été rendu ou présenté dans le texte source
    attribute rend {
      list {
        (
         ## spécifie que deux mots sont unis dans la source (sauf cas d'élision)
         "aggl"
         | 
           ## spécifie que, pour deux mots unis dans la source, le premier donne lieu à une élision
           "elision"
         | 
           ## spécifie que, pour deux mots séparés dans la source, le premier donne lieu à une élision
           "elision-sans-aggl"
         | 
           ## spécifie qu'un article, agrégé au mot précédent dans la source, donne lieu à une élision
           "enclise")+
