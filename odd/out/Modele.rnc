del =
  
  ## (suppression) contient une lettre, un mot ou un passage supprimé, marqué comme supprimé, sinon indiqué comme superflu ou erroné dans le texte par un auteur, un copiste, un annotateur ou un correcteur. [3.4.3. Additions, Deletions, and Omissions]
  element del {
    macro.paraContent,
    att.global.attribute.xmllang,
    att.global.linking.attribute.corresp,
    att.global.linking.attribute.synch,
    att.global.linking.attribute.copyOf,
    att.global.linking.attribute.next,
    att.global.linking.attribute.prev,
    att.global.linking.attribute.exclude,
    att.global.linking.attribute.select,
    att.global.analytic.attribute.ana,
    att.global.responsibility.attribute.cert,
    att.transcriptional.attribute.cause,
    att.transcriptional.attribute.seq,
    att.editLike.attribute.evidence,
    att.dimensions.attribute.extent,
    att.dimensions.attribute.precision,
    att.dimensions.attribute.scope,
    att.ranging.attribute.atLeast,
    att.ranging.attribute.atMost,
    att.ranging.attribute.confidence,
    att.source.attribute.source,
    
    ## (interprétation) indique comment l'élément en question a été rendu ou présenté dans le texte source
    attribute rend {
      list {
        (
         ## précise que la suppression est matérialisée par une rature
         "barré"
         | 
           ## précise que la suppression est matérialisée par une exponctuation
           "exponctué"
         | 
           ## précise que le texte supprimé a été gratté
           "gratté"
         | 
           ## précise que la suppression a été faite en repassant la séquence graphique concernée et en surimposant un nouveau texte. S'utilise généralement dans le cadre d'une combinaison avec subst.
           "repassé")+
      }
    }
    >> sch:pattern [
         id = "otinel-del-rend-repasse-only-in-subst-constraint-6"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:del/@rend"
           "\x{a}" ~
           "               "
           s:assert [
             test =
               "not(@rend='repassé') or (@rend='repassé' and parent::subst)"
             "la valeur 'repassé' n'a de sens que dans une\x{a}" ~
             "                                            substitution"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    empty
  }
